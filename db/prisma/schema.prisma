generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String    @id @unique
  name     String
  imageUrl String
  moodLog  MoodLog[]
}

model MoodLog {
  id Int @id @default(autoincrement())
  date   DateTime
  year   Int
  month  Int
  rating Int
  user   User     @relation(fields: [userId], references: [id])
  userId String
}
